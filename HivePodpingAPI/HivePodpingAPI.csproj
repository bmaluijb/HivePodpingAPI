<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
	  <PackageId>HivePodpingAPI</PackageId>
	  <Version>1.0.3</Version>
	  <Authors>Barry Luijbregts</Authors>
	  <Company>Podhome.fm</Company>
	  <Description>Interact with the Hive blockchain using the Hive JSON RPC API to get Podpings</Description>
	  <PackageTags>hive;podping;api;podcast</PackageTags>
	  <RepositoryUrl>https://github.com/bmaluijb/HivePodpingAPI</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Bcl.AsyncInterfaces" Version="8.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

</Project>
